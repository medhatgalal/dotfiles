{
  "awsAuthRefresh": "aws sso login --profile developer-eng-prod-access",
  "env": {
    "ANTHROPIC_MODEL": "global.anthropic.claude-sonnet-4-5-20250929-v1:0",
    "ANTHROPIC_SMALL_FAST_MODEL": "us.anthropic.claude-haiku-4-5-20251001-v1:0",
    "AWS_PROFILE": "ai-platform-dev.AdministratorAccess",
    "AWS_REGION": "us-east-1",
    "CLAUDE_CODE_FILE_READ_MAX_OUTPUT_TOKENS": "200000",
    "CLAUDE_CODE_MAX_OUTPUT_TOKENS": "16384",
    "CLAUDE_CODE_USE_BEDROCK": "1",
    "MAX_THINKING_TOKENS": "16384"
  },
  "hooks": {
    "PreCompact": [
      {
        "hooks": [
          {
            "command": "bd prime",
            "type": "command"
          }
        ],
        "matcher": ""
      }
    ],
    "SessionStart": [
      {
        "hooks": [
          {
            "command": "bd prime",
            "type": "command"
          }
        ],
        "matcher": ""
      }
    ]
  },
  "outputStyle": "default",
  "mcpServers": {
    "jira": {
      "command": "docker",
      "args": [
        "run",
        "--rm",
        "-i",
        "--env",
        "JIRA_EMAIL",
        "--env",
        "JIRA_TOKEN",
        "--env",
        "JIRA_URL",
        "${JIRA_MCP_IMAGE}"
      ],
      "env": {
        "JIRA_TOKEN": "${JIRA_TOKEN}",
        "JIRA_EMAIL": "${JIRA_EMAIL}",
        "JIRA_URL": "${JIRA_URL}"
      }
    },
    "gitlab": {
      "command": "docker",
      "args": [
        "run",
        "--rm",
        "-i",
        "--env",
        "GITLAB_TOKEN",
        "${GITLAB_MCP_IMAGE}"
      ],
      "env": {
        "GITLAB_TOKEN": "${GITLAB_TOKEN}"
      }
    },
    "github": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "-e",
        "GITHUB_PERSONAL_ACCESS_TOKEN",
        "${GITHUB_MCP_IMAGE}"
      ],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "${GITHUB_MCP_PAT}"
      }
    },
    "google-workspace": {
      "command": "uvx",
      "args": [
        "workspace-mcp"
      ],
      "env": {
        "GOOGLE_OAUTH_CLIENT_ID": "${GOOGLE_OAUTH_CLIENT_ID}",
        "GOOGLE_OAUTH_CLIENT_SECRET": "${GOOGLE_OAUTH_CLIENT_SECRET}",
        "USER_GOOGLE_EMAIL": "${USER_GOOGLE_EMAIL}"
      }
    },
    "context7": {
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp"
      ],
      "env": {
        "CONTEXT7_API_KEY": "${CONTEXT7_API_KEY}"
      }
    },
    "playwright": {
      "command": "npx",
      "args": [
        "--registry=https://registry.npmjs.org/",
        "-y",
        "@playwright/mcp"
      ]
    }
  },
  "statusLine": {
    "command": "~/.claude/statusline.sh",
    "padding": 0,
    "type": "command"
  }
}