{
  "experimental": {
    "useModelRouter": true,
    "skills": true,
    "plan": true,
    "enableAgents": true
  },
  "general": {
    "disableAutoUpdate": false,
    "enablePromptCompletion": true,
    "previewFeatures": true,
    "enableAutoUpdate": true,
    "defaultApprovalMode": "auto_edit"
  },
  "hooks": {
    "PreCompress": [
      {
        "hooks": [
          {
            "command": "bd prime",
            "type": "command"
          }
        ],
        "matcher": ""
      }
    ],
    "SessionStart": [
      {
        "hooks": [
          {
            "command": "bd prime",
            "type": "command"
          }
        ],
        "matcher": ""
      }
    ]
  },
  "mcpServers": {
    "context7": {
      "args": [
        "-y",
        "@upstash/context7-mcp",
        "--api-key",
        "$CONTEXT7_API_KEY"
      ],
      "command": "npx"
    },
    "github": {
      "headers": {
        "Authorization": "Bearer $GITHUB_MCP_PAT"
      },
      "httpUrl": "https://api.githubcopilot.com/mcp/"
    },
    "google-workspace": {
      "command": "$HOME/.gemini/mcp-servers/google-workspace/start.sh"
    },
    "jira": {
      "command": "docker",
      "args": [
        "run",
        "--rm",
        "-i",
        "--env",
        "JIRA_EMAIL",
        "--env",
        "JIRA_TOKEN",
        "--env",
        "JIRA_URL",
        "${JIRA_MCP_IMAGE}"
      ],
      "env": {
        "JIRA_TOKEN": "${JIRA_TOKEN}",
        "JIRA_EMAIL": "${JIRA_EMAIL}",
        "JIRA_URL": "${JIRA_URL}"
      }
    },
    "gitlab": {
      "command": "docker",
      "args": [
        "run",
        "--rm",
        "-i",
        "--env",
        "GITLAB_TOKEN",
        "${GITLAB_MCP_IMAGE}"
      ],
      "env": {
        "GITLAB_TOKEN": "${GITLAB_TOKEN}"
      }
    },
    "memory": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-memory"
      ],
      "env": {
        "MEMORY_FILE_PATH": "${HOME}/.mcp_shared_memory.json"
      }
    },
    "playwright": {
      "command": "npx",
      "args": [
        "--registry=https://registry.npmjs.org/",
        "-y",
        "@playwright/mcp"
      ]
    }
  },
  "output": {
    "format": "text"
  },
  "security": {
    "auth": {
      "selectedType": "vertex-ai"
    },
    "environmentVariableRedaction": {
      "enabled": true
    },
    "enablePermanentToolApproval": true
  },
  "tools": {
    "shell": {
      "showColor": true
    },
    "autoAccept": true,
    "approvalMode": "auto_edit"
  },
  "ui": {
    "showMemoryUsage": true,
    "showStatusInTitle": true,
    "showModelInfoInChat": true
  }
}
